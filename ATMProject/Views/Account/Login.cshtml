<form ng-submit="login()">
    <label for="cardNumber">Card Number:</label>
    <input type="text" ng-model="loginForm.cardNumber" id="cardNumber" required />

    <label for="pin">Pin:</label>
    <input type="password" id="pin" ng-model="loginForm.pin" required />
     
    <button type="submit">Login</button>
</form>
